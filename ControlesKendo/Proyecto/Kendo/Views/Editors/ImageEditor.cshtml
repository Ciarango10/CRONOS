<head>
    <base href="https://demos.telerik.com/kendo-ui/imageeditor/index">
    <style>
        html {
            font-size: 14px;
            font-family: Arial, Helvetica, sans-serif;
        }</style>
    <title></title>
    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/2022.3.1109/styles/kendo.default-ocean-blue.min.css" />
    <script src="https://kendo.cdn.telerik.com/2022.3.1109/js/jquery.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2022.3.1109/js/kendo.all.min.js"></script>
    <link rel="stylesheet" href="../content/shared/styles/examples-offline.css">
    <script src="../content/shared/js/console.js"></script>
</head>
<body>
    <div id="example">
        <div class="demo-section wide">
            <div id="imageEditor"></div>
        </div>

        <script>
            $(document).ready(function () {
                $("#imageEditor").kendoImageEditor({
                    imageUrl: "../content/shared/images/photos/2.jpg",
                    width: "100%",
                    height: 900,
                    saveAs: {
                        fileName: "image_edited.png"
                    }
                });

                var imageEditor = $("#imageEditor").getKendoImageEditor();
                imageEditor.one("imageRendered", function () {
                    imageEditor.executeCommand({ command: "ZoomImageEditorCommand", options: imageEditor.getZoomLevel() - 0.2 });
                });
            });
        </script>
    </div>

    <div id="example">
        <div class="configurator">
            <div class="header">Configurator</div>
            <div class="box-col">
                <h4>Options</h4>
                <ul class="options">
                    <li><button id="drawImage">Draw Image</button></li>
                    <li><button id="drawCanvas">Draw Canvas</button></li>
                    <li><button id="zoomLevel">Get Zoom Level</button></li>
                </ul>
            </div>
            <div class="box-col">
                <h4>Commands</h4>
                <ul class="options">
                    <li><button id="save">Save</button></li>
                    <li><button id="open">Open</button></li>
                </ul>
            </div>
        </div>

        <div class="demo-section wide">
            <div id="imageEditor2"></div>
        </div>
        <script>
            $(document).ready(function () {
                var base64SampleImage = "data:image/jpeg;base64,"

                var drawImage = function (e) {
                    if (e.type != "keypress" || kendo.keys.ENTER == e.keyCode) {
                        imageEditor.drawImage(base64SampleImage).done(function (image) {
                            imageEditor.drawCanvas(image);
                            imageEditor._initUndoRedoStack();
                            imageEditor._toggleTools();
                        }).fail(function (args) {
                            alert("Something went wrong!");
                        });
                    }
                },
                drawCanvas = function (e) {
                    if (e.type != "keypress" || kendo.keys.ENTER == e.keyCode) {
                        imageEditor.drawImage("../content/shared/images/photos/5.jpg").done(function (image) {
                            imageEditor.drawCanvas(image);
                            imageEditor._initUndoRedoStack();
                            imageEditor._toggleTools();
                        }).fail(function (args) {
                            alert("Something went wrong!");
                        });
                    }
                },
                getZoom = function (e) {
                    if (e.type != "keypress" || kendo.keys.ENTER == e.keyCode) {
                        kendo.alert('Curent zoom level: ' + imageEditor.getZoomLevel());
                    }
                },
                open = function (e) {
                    if (e.type != "keypress" || kendo.keys.ENTER == e.keyCode) {
                        imageEditor.executeCommand({ command: "OpenImageEditorCommand" })
                    }

                },
                save = function (e) {
                    if (e.type != "keypress" || kendo.keys.ENTER == e.keyCode) {
                        imageEditor.executeCommand({ command: "SaveImageEditorCommand", options: {fileName:"myEditedImage.jpg"} })
                    }
                };

                var imageEditor = $("#imageEditor2").kendoImageEditor({
                        imageUrl: "../content/shared/images/photos/11.jpg",
                        width: "100%",
                        height: 900,
                        saveAs: {
                            fileName: "image_edited.png"
                        }
                    }).data("kendoImageEditor");

                $("#drawImage").click(drawImage);
                $("#drawImage").keypress(drawImage);

                $("#drawCanvas").click(drawCanvas);
                $("#drawCanvas").keypress(drawCanvas);

                $("#zoomLevel").click(getZoom);
                $("#zoomLevel").keypress(getZoom);

                $("#open").click(open);
                $("#open").keypress(open);

                $("#save").click(save);
                $("#save").keypress(save);
            });
        </script>
    </div>

    <div id="example">
        <div class="demo-section wide">
            <div id="imageEditor3"></div>
        </div>

        <script>
            $(document).ready(function () {
                function onError(e) {
                    kendoConsole.log('Error')
                };

                function onImageLoaded(e) {
                    kendoConsole.log('Image loaded')
                };

                function onImageRendered(e) {
                    kendoConsole.log('Image rendered')
                };

                function onExecute(e) {
                    kendoConsole.log(e.command + ' command executed')
                };

                $("#imageEditor3").kendoImageEditor({
                    imageUrl: "../content/shared/images/photos/10.jpg",
                    width: "100%",
                    height: 900,
                    saveAs: {
                        fileName: "image_edited.png"
                    },
                    error: onError,
                    imageLoaded: onImageLoaded,
                    imageRendered: onImageRendered,
                    execute: onExecute
                });
            });
        </script>
    </div>

    <div class="kd-example-console">
        <div class="header">
            <span class="title">CONSOLE LOG</span>
            <span class="clear kd-text-secondary">Clear log</span>
        </div>
        <div class="console"></div>
    </div>

    <div id="example">
        <div class="demo-section wide">
            <div id="imageEditor4"></div>
        </div>

        <script>
            $(document).ready(function () {
                $("#imageEditor4").kendoImageEditor({
                    imageUrl: "../content/shared/images/photos/12.jpg",
                    width: "100%",
                    height: 900,
                    saveAs: {
                        fileName: "image_edited.png"
                    }
                });
            });

            $(document).on("keydown", function (e) {
                if (e.altKey && e.keyCode === 87) {
                    $("#imageEditor4").data("kendoImageEditor").wrapper.focus();
                }
            });
        </script>
    </div>
</body>
