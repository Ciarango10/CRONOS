<body>

    <div class="k-d-flex k-justify-content-center" style="padding-top: 54px;">
        <div class="k-w-300">
            <ul id="fieldlist">
                <li>
                    <input id="phone_number" value="555 123 4567" />
                </li>
                <li>
                    <input id="credit_card" value="1234 1234 1234 1234" />
                </li>
                <li>
                    <input id="ssn" value="003-12-3456" />
                </li>
                <li>
                    <input id="postcode" value="W1N 1AC" />
                </li>
            </ul>
        </div>
    </div>

    <script>
        $(document).ready(function () {
            $("#phone_number").kendoMaskedTextBox({
                label: "Phone number:",
                mask: "(999) 000-0000"
            });

            $("#credit_card").kendoMaskedTextBox({
                label: "Credit Card number:",
                mask: "0000 0000 0000 0000"
            });

            $("#ssn").kendoMaskedTextBox({
                label: "Social security number:",
                mask: "000-00-0000"
            });

            $("#postcode").kendoMaskedTextBox({
                label: "UK postcode:",
                mask: "L0L 0LL"
            });
        });
    </script>

    <style>
        #fieldlist {
            margin: 0 0 -2em;
            padding: 0;
        }

            #fieldlist li {
                list-style: none;
                padding-bottom: 2em;
            }
    </style>

    <div class="configurator">
        <div class="header">Configurator</div>
        <div class="box-col">
            <h4>SIZE</h4>
            <ul class="options">
                <li>
                    <input id="size" />
                </li>
            </ul>
        </div>
        <div class="box-col">
            <h4>BORDER RADIUS</h4>
            <ul class="options">
                <li>
                    <input id="rounded" />
                </li>
            </ul>
        </div>
        <div class="box-col">
            <h4>FILL MODE</h4>
            <ul class="options">
                <li>
                    <input id="fillMode" />
                </li>
            </ul>
        </div>
    </div>

    <div class="k-d-flex k-justify-content-center" style="padding-top: 54px;">
        <div class="k-w-300">
            <input id="phone_number2" value="555 123 4567" />
        </div>
    </div>
    <script>
        $(document).ready(function () {
            var maskedtextbox = $("#phone_number2").kendoMaskedTextBox({
                mask: "(999) 000-0000",
                label: "Value"
            }).data('kendoMaskedTextBox');

            $("#size").kendoDropDownList({
                dataSource: ["small", "medium", "large", "none"],
                value: "medium",
                change: function () {
                    let value = this.value();
                    maskedtextbox.setOptions({ size: value });
                }
            });

            $("#rounded").kendoDropDownList({
                dataSource: ["small", "medium", "large", "full", "none"],
                value: "medium",
                change: function () {
                    let value = this.value();
                    maskedtextbox.setOptions({ rounded: value });
                }
            });

            $("#fillMode").kendoDropDownList({
                dataSource: ["solid", "flat", "outline", "none"],
                value: "solid",
                change: function () {
                    let value = this.value();
                    maskedtextbox.setOptions({ fillMode: value });
                }
            });
        });
    </script>

    <style>
        .k-picker.k-dropdownlist {
            width: 140px;
        }
    </style>

