    <div class="k-d-flex k-justify-content-center" style="padding-top: 54px;">
		<div class="k-w-300">
			<input id="maskedtextbox4" value="555 123 4567" />
		</div>
    </div>

    <script>
        $(document).ready(function() {
            $("#maskedtextbox4").kendoMaskedTextBox({
                mask: "(999) 000-0000",
                label: {
                    content: "Set phone number",
                    floating: true
                }
