            position: relative;
            width: 650px;
            height: 477px;
            margin: 0 auto;
            padding: 0;
            list-style-type: none;
        }

            #book4 > li {
                position: absolute;
                width: 100%;
                height: 100%;
                background-color: #fff;
            }

        #actions4 {
            overflow: hidden;
            margin: 0 auto 3em;
            width: 650px;
        }

        #previous4, #next4 {
            text-decoration: none;
            text-indent: -999em;
            overflow: hidden;
            display: block;
            height: 100%;
            width: 90px;
            position: absolute;
            top: 0;
            background-position: 50% 50%;
            opacity: .5;
            background-color: red
        }

            #previous4:hover, #next4:hover {
                opacity: 1;
            }

        #previous4 {
            left: 0;
            background-color: red
        }

        #next4 {
            right: 0;
            background-color: black
        }

        .first-page #previous4,
        .last-page #next4 {
            display: none;
        }
    </style>
</div>

