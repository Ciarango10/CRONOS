                                ] },
                                { text: "Item 3" }
                            ]
                        },
                        loadOnDemand: false
                    }).data("kendoTreeView"),

                    handleTextBox = function(callback) {
                        return function(e) {
                            if (e.type != "keypress" || kendo.keys.ENTER == e.keyCode) {
                                callback(e);
                            }
                        };
                    };


                $("#disableNode15").click(function() {
                    var selectedNode = treeview.select();

                    treeview.enable(selectedNode, false);
                });

                $("#enableAllNodes15").click(function() {
                    var selectedNode = treeview.select();

                    treeview.enable(".k-item");
                });

                $("#removeNode15").click(function() {
                    var selectedNode = treeview.select();

                    treeview.remove(selectedNode);
                });

                $("#expandAllNodes15").click(function() {
                    treeview.expand(".k-item");
                });

                $("#collapseAllNodes15").click(function() {
                    treeview.collapse(".k-item");
                });

                var append = handleTextBox(function(e) {
                        var selectedNode = treeview.select();

                        // passing a falsy value as the second append() parameter
                        // will append the new node to the root group
                        if (selectedNode.length == 0) {
                            selectedNode = null;
                        }

                        treeview.append({
                            text: $("#appendNodeText15").val()
                        }, selectedNode);
                    });

                $("#appendNodeToSelected15").click(append);
                $("#appendNodeText5").keypress(append);

                // datasource actions

                var ascending = false;

                $("#sortDataSource15")
                    .text(ascending ? "Sort ascending" : "Sort descending")
                    .click(function() {
                        treeview.dataSource.sort({
                            field: "text",
                            dir: ascending ? "asc" : "desc"
                        });

                        ascending = !ascending;

                        $(this).text(ascending ? "Sort ascending" : "Sort descending")
                    });

                var filter = handleTextBox(function(e) {
                    var filterText = $("#filterText15").val();

                    if (filterText !== "") {
                        treeview.dataSource.filter({
                            field: "text",
                            operator: "contains",
                            value: filterText
                        });
                    } else {
                        treeview.dataSource.filter({});
                    }
                });

                $("#filterDataSource15").click(filter);
                $("#filterText15").keypress(filter);
            });
